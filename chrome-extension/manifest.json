{
	"name": "Ariel予定通知",
	"version": "0.1.1",
	"manifest_version": 2,
	"description": "",
	"icons": {
		"128": "/icon/icon.png"
	},
	"permissions": [
		"alarms",
		"storage",
		"notifications"
	],
	"optional_permissions": [
		"http://*/",
		"https://*/"
	],
	"options_page": "setting/index.html",
	"browser_action": {
		"default_popup": "schedule/index.html"
	},
	"background": {
		"persistent": false,
		"scripts": [
			"js/Schedule.js",
			"background.js"
		]
	}
}
